{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "paths": {
      "@/*": ["./src/*"]
    },
    "rootDir": "./src",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
  },
  "extends": "../tsconfig.json",
  "include": ["src/**/*"]
}
